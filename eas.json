{
  "cli": {
    "version": ">= 10.2.0"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development",
      "env": {
        "API_URL": "https://api.development.com",
        "API_ACCESS_TOKEN": "<your_development_access_token>"
      }
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview",
      "env": {
        "API_URL": "",
        "API_ACCESS_TOKEN": ""
      }
    },
    "production": {
      "channel": "production",
      "env": {
        "API_URL": "",
        "API_ACCESS_TOKEN": ""
      }
    }
  },
  "submit": {
    "production": {}
  }
}